<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <template id="assets_backend" name="ui-translation assets" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <script type="text/javascript" src="/ui_translation/static/src/js/ui_translation.js"></script>
            </xpath>
        </template>
        <template id="qunit_suite" name="ui-translation qunit-assets" inherit_id="web.qunit_suite">
            <xpath expr="//head" position="inside">
                <script type="text/javascript" src="/ui_translation/static/tests/test_ui_translation.js"></script>
            </xpath>
        </template>
    </data>
</openerp>
